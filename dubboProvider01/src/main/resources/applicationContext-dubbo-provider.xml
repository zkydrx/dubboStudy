<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <dubbo:application name="dubbo-provider01"/>


    <!--&lt;!&ndash; zookeeper注册中心配置写法一 &ndash;&gt;-->
    <!--<dubbo:registry address="zookeeper://35.189.178.24:2181"/>-->

    <!-- zookeeper注册中心配置写法二 -->
    <dubbo:registry protocol="zookeeper" address="35.189.178.24:2181"/>



    <bean id="helloDubbo" class="com.htt.dubbo.impl.HelloDubboServiceImpl"/>

    <!--<dubbo:service interface="com.htt.dubbo.HelloDubboService" ref="helloDubbo" registry="N/A"/>-->
    <dubbo:service interface="com.htt.dubbo.HelloDubboService" ref="helloDubbo"/>

</beans>