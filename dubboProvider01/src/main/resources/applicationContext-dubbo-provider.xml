<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <dubbo:application name="dubbo-provider01"/>


    <dubbo:registry address="multicast://224.3.4.5:1234"/>
    <bean id="helloDubbo" class="com.htt.dubbo.impl.HelloDubboServiceImpl"/>

    <!--<dubbo:service interface="com.htt.dubbo.HelloDubboService" ref="helloDubbo" registry="N/A"/>-->
    <dubbo:service interface="com.htt.dubbo.HelloDubboService" ref="helloDubbo"/>

</beans>